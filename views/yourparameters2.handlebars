<div id="myDIV" class="header">
    <div class="container">
        <h2 style="margin:5px">My Parameters</h2>
        <form action="add3">
            <div class="row">
                <div class="col-xs-6">
                    <input class="form-control" type="text" id="myInput" name="newtype" placeholder="Parameter Type" required>
                </div>
                <div class="col-xs-6">
                    <input class="form-control" type="text" id="myInput2" name="newname" placeholder="Parameter Name" required>
                </div>
            </div>
            <br>
            <div>
                <button type="submit" class="btn btn-lg btn-block" id="myInput3" name="submit">add</button>
            </div>
        </form>
    </div>
</div>
<div class="container">
    <h1>Here is a list of your inputted parameters:</h1>

    <ul>
        {{#each parameters}}
            <li>
                <h3>{{type}}, {{name}}</h3>
            </li>
        {{/each}}
    </ul>
</div>
