<script>

$.get("/getSuggs",cbf);

function cbf(result) {
  var initialCodeArray = [];
  var suggArray = [];
  var initialCodeArrayString = "";
  var suggArrayString = "";

 for (var c = 0; c < sessionStorage.length; c++){
    for (var j = 0; j < result.sugg.length; j++) {
        if ( sessionStorage.getItem('key'+c).includes(result.sugg[j].english) ) {
          initialCodeArrayString += sessionStorage.getItem('key'+c) + '<br>';
          suggArrayString += result.sugg[j].suggestions + '<br>';
          console.log("why printing twice" + result.sugg[j].suggestions);
        }    
      }
    
  }

    //document.getElementById('initialCodeArray').innerHTML = initialCodeArray.toString();
    //document.getElementById('suggArray').innerHTML = suggArray.toString();
    $("#suggArray").html(suggArrayString);
    $("#initialCodeArray").html(initialCodeArrayString);

 }
</script>

